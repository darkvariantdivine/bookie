{
  "name": "bookie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev-reset": "rm -rf .next .vscode dist",
    "dev-be-build": "poetry build",
    "dev-be-start": "python3 -m bookie",
    "docker-build": "docker-compose build",
    "docker-setup": "mkdir -p logs && mkdir -p ssl && cd ssl && openssl req -x509 -nodes -days 36500 -newkey rsa:2048 --keyout bookie.key -out bookie.crt -subj '/C=SG/L=Singapore/O=Bookie/CN=bookie'",
    "docker-start": "mkdir -p logs && docker-compose up -d",
    "docker-stop": "docker-compose down",
    "docker-reset": "docker-compose down -v",
    "docker-reset-full": "docker-compose down -v && rm -rf logs ssl",
    "tests-unit": "jest",
    "tests-preview": "jest-preview",
    "tests-postman": "docker-compose -f docker-compose.test.yml up -d && newman run postman/Bookie.postman_collection.json -e postman/environments/Docker.postman_environment.json || exit 1 && docker exec bookie_mongo mongo setup/bookie.js && docker-compose -f docker-compose.test.yml down -v"
  },
  "dependencies": {
    "@mantine/carousel": "^5.10.4",
    "@mantine/core": "^5.10.4",
    "@mantine/dates": "^5.10.4",
    "@mantine/form": "^5.10.4",
    "@mantine/hooks": "^5.10.4",
    "@mantine/modals": "^5.10.4",
    "@mantine/next": "^5.10.4",
    "@mantine/notifications": "^5.10.4",
    "@mantine/utils": "^5.10.4",
    "@next/font": "13.1.6",
    "@tabler/icons": "^1.119.0",
    "@types/node": "18.14.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "embla-carousel-autoplay": "^7.0.9",
    "embla-carousel-react": "^7.0.9",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "mantine-react-table": "^0.9.3",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5",
    "validator": "^13.9.0",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@playwright/test": "^1.31.2",
    "@tanstack/react-query-devtools": "^4.26.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "jest": "^29.5.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-preview": "^0.3.1",
    "newman": "^5.3.2",
    "playwright": "^1.31.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2"
  }
}
