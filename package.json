{
  "name": "bookie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "unittests": "jest",
    "reset": "rm -rf ssl .next",
    "docker-build": "docker-compose build",
    "docker-start": "mkdir -p logs && docker-compose up -d",
    "docker-stop": "docker-compose down",
    "docker-reset": "docker-compose down -v",
    "gen-certs": "mkdir -p ssl && cd ssl && openssl req -x509 -nodes -days 36500 -newkey rsa:2048 --keyout bookie.key -out bookie.crt -subj '/C=SG/L=Singapore/O=Bookie/CN=bookie'"
  },
